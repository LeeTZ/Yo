#For those machine doesn't have ocamlbuild, build the project with this makefile
#	
OBJ=ast.cmo \
	scanner.cmo \
	parser.cmo \
	parser_test.cmo \
	sast.cmo \
	built_in_type.cmo \
	semantic.cmo \
	type_reader.cmo \
	print_typetab.cmo  \
	typereader_test.cmo \
	generate_test.cmo
	
YO=typereader_test

FLAGS:=-g

$(YO): $(OBJ)
	#ocamlc -g -o scanner_test scanner.cmo
	#ocamlc -g -o parser_test parser.cmo scanner.cmo ast.cmo parser_test.cmo 
	#ocamlc -g -o generate_test parser.cmo scanner.cmo ast.cmo sast.cmo semantic.cmo built_in_type.cmo type_reader.cmo print_typetab.cmo 
	ocamlc -g -o typereader_test parser.cmo scanner.cmo ast.cmo sast.cmo semantic.cmo built_in_type.cmo type_reader.cmo print_typetab.cmo typereader_test.cmo 
	
.SUFFIXES: .ml .cmo .cmi .mll .mly .mli
.PRECIOUS: %.ml %.mli %.cmo

.ml.cmo:
	ocamlc -c $(FLAGS) $<

.mli.cmi:
	ocamlc -c $(FLAGS) $<
    
.mll.ml:
	ocamllex $<

.mly.ml:
	ocamlyacc -v $<

.mly.mli:
	ocamlyacc -v $<

clean:
	rm -f *.cmi *.cmo parser.ml scanner.ml *.output parser.mli parser_test semantic_test generate_test 



